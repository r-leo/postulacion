   "source": [
    "# Calcular el PIB agregado de África comoa la suma de África oriental y occidental:\n",
    "africa = pd.DataFrame({\n",
    "    'País': 'África',\n",
    "    'Clave': 'AFR'\n",
    "} | {str(año): pib_anual.query(\"`Clave` == 'AFE'\")[str(año)].values + pib_anual.query(\"`Clave` == 'AFW'\")[str(año)].values for año in range(2011, 2021)})\n",
    "\n",
    "# Añadir el agregado de África a la base de datos:\n",
    "pib_anual = pib_anual.append(africa)\n",
    "\n",
    "# Transformar los datos de formato wide a long:\n",
    "pib_anual = pib_anual.melt(id_vars = ['País', 'Clave'], var_name = 'Año', value_name = 'PIB')\n",
    "\n",
    "# Convertr la variable de años a números enteros:\n",
    "pib_anual['Año'] = pd.to_numeric(pib_anual['Año'])\n",
    "\n",
    "# Calcular el crecimiento del PIB anual:\n",
    "pib_anual['Crecimiento'] = pib_anual['PIB'].groupby([pib_anual['País'], pib_anual['Clave']]).transform(lambda x: (x - x.shift()) / x.shift() * 100)"
   ]
