   "metadata": {},
   "source": [
    "Para el análisis de indicadores de corto plazo utilizamos la base de datos `indicadores_corto_plazp.csv`:"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 255,
   "id": "6a52dd51-7e5d-4cf5-b89e-db9f3bc06f19",
   "metadata": {},
   "outputs": [],
   "source": [
    "indicadores = pd.read_csv('indicadores_corto_plazo.csv')\n",
    "indicadores = indicadores.melt(id_vars = 'Indicador', var_name = 'Periodo', value_name = 'Índice')\n",
    "indicadores['Fecha'] = indicadores['Periodo'].apply(lambda x: x.split('M')[0] + \"-\" + x.split('M')[1] + \"-1\")\n",
    "indicadores['Fecha'] = pd.to_datetime(indicadores['Fecha'])\n",
    "indicadores = indicadores.sort_values('Fecha')\n",
    "indicadores['Crecimiento respecto al mes anterior'] = indicadores['Índice'].groupby(indicadores['Indicador']).transform(lambda x: (x - x.shift(periods=1)) / x.shift(periods=1) * 100)\n",
    "indicadores['Crecimiento respecto al mismo mes del año anterior'] = indicadores['Índice'].groupby(indicadores['Indicador']).transform(lambda x: (x - x.shift(periods=12)) / x.shift(periods=12) * 100)\n",
    "indicadores = indicadores.dropna()"
   ]
  },
