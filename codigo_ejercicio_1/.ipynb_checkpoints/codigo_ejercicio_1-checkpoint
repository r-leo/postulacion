     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "g = sns.relplot(\n",
    "    x = 'Fecha',\n",
    "    y = 'Inflación mensual',\n",
    "    kind = 'line',\n",
    "    hue = 'País',\n",
    "    style = 'País',\n",
    "    markers = True,\n",
    "    dashes = False,\n",
    "    ci = None,\n",
    "    data = ipc.query(\"País in ('México', 'Estados Unidos') & Fecha >= '2019-01-01'\")\n",
    ").set(\n",
    "    xlabel = 'Año',\n",
    "    ylabel = 'Inflación mensual (%)'\n",
    ")\n",
    "\n",
    "g.ax.set_xticklabels(g.ax.get_xticklabels(), rotation = 90)\n",
